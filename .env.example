# PluviaX Weather AI - Environment Variables
# Copy this file to .env and fill in your API keys

# Meteomatics API (Professional Weather Data)
METEOMATICS_USERNAME=your_meteomatics_username
METEOMATICS_PASSWORD=your_meteomatics_password

# OpenWeatherMap API (Current Weather & Forecasts)
OPENWEATHER_API_KEY=your_openweather_api_key

# DeepSeek AI API (AI Weather Analysis)
DEEPSEEK_API_KEY=your_deepseek_api_key

# Server Configuration
PORT=3000
NODE_ENV=development
